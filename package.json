{
  "name": "geocortex-dev-center",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "lint": "npm run lint:links && npm run lint:spelling && npm run prettier -- --check",
    "lint:spelling": "cspell \"{docs,src}/**\"",
    "lint:links": "remark -f -q -u validate-links \"docs/**/*.mdx\"",
    "prettier": "prettier \"**/*.{css,js,jsx,ts,tsx,md,mdx,json}\"",
    "prettier:write": "npm run prettier -- --write",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.50",
    "@docusaurus/preset-classic": "2.0.0-alpha.50",
    "classnames": "^2.2.6",
    "cspell": "^4.0.57",
    "husky": "^4.2.3",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remark-cli": "^8.0.0",
    "remark-validate-links": "^10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
